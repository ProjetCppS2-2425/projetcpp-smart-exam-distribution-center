        <property name="styleSheet">
         <string notr="true">QPushButton {
    color: rgb(51, 102, 153); /* Texte bleu pour correspondre à la bordure */
    background-color: white; /* Fond blanc pour l'intérieur du bouton */
    font-size: 18px;
    line-height: 1.5;
    width: 200px;
    height: 40px;
    opacity: 0.85; /* Légèrement plus opaque pour un effet sophistiqué */
    text-align: center;
    
    border-style: solid;
    border-width: 2px; 
    border-color: rgb(51, 102, 153); /* Bordure bleue, pour un contraste élégant */
    
    border-radius: 8px; /* Bordure légèrement arrondie pour un effet doux */
    padding: 8px 12px; /* Un peu d'espacement pour améliorer le confort */
    
    transition: all 0.3s ease; /* Pour une transition fluide des effets */
}

/* Effet de survol */
QPushButton:hover {
    background-color: rgb(240, 248, 255); /* Légère teinte bleue pour le survol */
    border-color: rgb(30, 95, 150); /* Changement de bordure au survol */
    color: rgb(30, 95, 150); /* Texte bleu au survol */
    cursor: pointer; /* Curseur en forme de main */
}

/* Effet lorsque le bouton est pressé */
QPushButton:pressed {
    background-color: rgb(230, 240, 255); /* Teinte encore plus claire au clic */
    border-color: rgb(20, 80, 130); /* Bordure plus foncée */
    color: rgb(20, 80, 130); /* Texte plus foncé lors du clic */
}
