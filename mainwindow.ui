        <property name="styleSheet">
         <string notr="true">/* Style de la table QTableWidget */
QTableWidget {
    background-color: #ffffff; /* Fond blanc pour la table */
    border: 2px solid #2980b9; /* Bordure bleue pour délimiter la table */
    border-radius: 8px; /* Coins arrondis pour une présentation douce */
    padding: 10px; /* Espacement interne pour éviter que le contenu touche les bords */
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Ombre discrète pour ajouter de la profondeur */
    font-size: 14px; /* Taille de la police pour le contenu de la table */
    color: rgb(51, 102, 153); /* Couleur du texte pour la lisibilité */
}

/* Style pour les lignes de la table (QTableWidget::item) */
QTableWidget::item {
    padding: 12px; /* Espacement des cellules */
    border-bottom: 1px solid #f0f0f0; /* Ligne de séparation entre les lignes */
    background-color: #ffffff; /* Fond blanc pour chaque cellule */
    transition: background-color 0.3s ease; /* Transition douce pour le survol */
}

/* Effet de survol sur les cellules */
QTableWidget::item:hover {
    background-color: rgb(240, 248, 255); /* Teinte bleue légère sur survol */
    cursor: pointer; /* Curseur en forme de main */
}

/* Style des cellules sélectionnées */
QTableWidget::item:selected {
    background-color: rgb(51, 102, 153); /* Fond bleu pour la cellule sélectionnée */
    color: white; /* Texte blanc pour une bonne visibilité */
    border-radius: 8px; /* Coins arrondis pour la cellule sélectionnée */
}

/* Style des entêtes de colonnes */
QHeaderView::section {
    background-color: #f7f9fc; /* Fond clair pour les entêtes */
    color: rgb(51, 102, 153); /* Couleur du texte des entêtes */
    font-weight: bold; /* Mettre en gras les entêtes */
    padding: 10px; /* Espacement autour du texte des entêtes */
    border: 1px solid #dcdfe6; /* Bordure légère autour des entêtes */
    text-align: center; /* Alignement centré du texte */
}

/* Style des boutons dans les entêtes de colonnes */
QHeaderView::section:hover {
    background-color: rgb(240, 248, 255); /* Teinte bleue légère au survol des entêtes */
    cursor: pointer; /* Curseur en forme de main */
}
</string>
