        <property name="styleSheet">
         <string notr="true">QLineEdit {
    color: rgb(51, 102, 153); /* Texte bleu */
    background-color: white; /* Fond blanc */
    font-size: 18px; /* Taille de police */
    border: 2px solid rgb(51, 102, 153); /* Bordure bleue */
    border-radius: 8px; /* Coins arrondis */
    padding: 8px 12px; /* Espacement à l'intérieur */
    transition: all 0.3s ease; /* Transition fluide */
}

/* Effet de focus (lorsque l'utilisateur clique dans le champ) */
QLineEdit:focus {
    border-color: rgb(30, 95, 150); /* Bordure bleue au focus */
    background-color: rgb(240, 248, 255); /* Fond légèrement bleu clair */
    color: rgb(30, 95, 150); /* Texte bleu au focus */
}

/* Effet au survol (lorsque l'utilisateur passe la souris sur la barre de recherche) */
QLineEdit:hover {
    border-color: rgb(30, 95, 150); /* Bordure bleue au survol */
    background-color: rgb(240, 248, 255); /* Légère teinte bleue au survol */
    cursor: pointer; /* Curseur de la souris sous forme de main */
}

/* Lorsque l'utilisateur commence à taper (focus + survol) */
QLineEdit:active {
    background-color: rgb(230, 240, 255); /* Fond encore plus clair */
}

/* Pour le texte d'exemple (placeholder) */
QLineEdit::placeholder {
    color: rgb(150, 150, 150); /* Texte gris pour le placeholder */
    font-style: italic; /* Style italique pour le placeholder */
}
