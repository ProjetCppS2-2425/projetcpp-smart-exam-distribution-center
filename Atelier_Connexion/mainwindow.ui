     <string notr="true">/* === Background with Linear Gradient === */
QWidget {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, 
                                stop:0 #007BFF, stop:1 #F8F9FA);
    font-family: &quot;Segoe UI&quot;, sans-serif;
    font-size: 12pt;
}

/* === Group Box Styling === */
QGroupBox {
    border: 2px solid #0056b3;
    border-radius: 10px;
    margin-top: 10px;
    padding: 15px;
    background: transparent; /* Semi-transparent */
}

/* === Stylish Input Fields === */
QLineEdit, QComboBox, QDateEdit {
    border: 2px solid #007BFF;
    border-radius: 6px;
    padding: 6px;
    background: white;
    transition: all 0.3s ease-in-out;
}

/* === Input Fields on Focus === */
QLineEdit:focus, QComboBox:focus, QDateEdit:focus {
    border: 2px solid #0056b3;
    background: #e6f2ff;
}

/* === Modern Buttons === */
QPushButton {
    background-color: #007BFF;
    color: white;
    border-radius: 10px;
    padding: 10px 20px;
    font-weight: bold;
    border: none;
    transition: all 0.3s ease-in-out;
}

/* === Button Hover Effect === */
QPushButton:hover {
    background-color: #0056b3;
    transform: scale(1.05);
}

/* === Button Click Effect === */
QPushButton:pressed {
    background-color: #003d82;
    transform: scale(0.95);
}

/* === Table Styling === */
QTableWidget {
    background: white;
    border: 2px solid #007BFF;
    gridline-color: #007BFF;
    selection-background-color: #CDE1F8;
}

/* === Headers in Table === */
QHeaderView::section {
    background-color: #007BFF;
    color: white;
    font-weight: bold;
    padding: 5px;
    border: 1px solid #0056b3;
}

/* === Scrollbar for Table === */
QScrollBar:vertical, QScrollBar:horizontal {
    background: white;
    border: 2px solid #007BFF;
    width: 12px;
    border-radius: 6px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background: #007BFF;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background: #0056b3;
}

QLabel{
background-color:transparent;
}

QFormLayout{
background-color:transparent;
}</string>
